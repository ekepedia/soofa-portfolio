<div class="">

    <div class="col-xs-4 left-column">
        <p class="inbox" style="margin-left: -20px; display: none; font-size: 12px;letter-spacing: 2px;">
            <span style="margin-left: 80px; margin-right: 10%">COMPOSE</span>
            <span style="font-size: 20px">|</span>
            <span class="selected-toggle">INBOX </span>
            <span ng-click="reload()" style="float: right;margin-right: 20px;font-size: 12px;margin-top: 10px;" class="glyphicon glyphicon-repeat" aria-hidden="true"></span>
        </p>

        <section class="login-pane">
            <form ng-submit="login()">
                <div class="login-logo-holder">
                    <img class="login-logo" src="/img/logo.png"/>

                </div>
                <div class="form-group">
                    <input type="email" class="form-control minimum-input" ng-model="email" placeholder="Email">
                </div>
                <div class="form-group">
                    <input type="password" class="form-control minimum-input" ng-model="password" placeholder="Password">
                </div>
                <div class="form-group minimum-submit-wrapper">
                    <button type="submit" class="minimum-submit">LOGIN</button>

                </div>
            </form>
        </section>

        <section class="messages-pane" style="display: none">
            <div ng-repeat="m in messages" ng-click="set_messages(m)" ng-class="{selected : is_selected(m)}">

                <div class="row contact-panel">
                    <div style="margin-top: 34px">
                        <div class="contact-photo-holder">
                            <img preload src="{{get_photo(get_inbox_contact(m))}}" class="contact-photo img-circle">
                        </div>
                        <div class="contact-name-holder">
                            <p>{{get_name(get_inbox_contact(m))}}</p>
                            <p class="company">{{get_company(get_inbox_contact(m))}}</p>
                        </div>
                        <div class="">
                            <div class="unread-count contact-unread-holder" ng-if="count_unread(m) > 0">
                                <p>{{count_unread(m)}} New</p>
                            </div>
                            <div class="time-stamp contact-unread-holder" ng-if="count_unread(m) === 0">
                                <p>{{date(m[0].created_at)}}</p>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </section>

    </div>

    <div class="col-xs-8  right-column">
        <div class="video-loading" style="display: none">
            <svg class="lds-spin" width="100%" height="100%" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 100 100" preserveAspectRatio="xMidYMid"><g transform="translate(66,50)">
                    <g transform="rotate(0)">
                        <circle cx="0" cy="0" r="4" fill="#5B3DDE" fill-opacity="1" transform="scale(0.3 0.3)">
                            <animateTransform attributeName="transform" type="scale" begin="-0.9s" values="0 0;1 1" keyTimes="0;1" dur="1s" repeatCount="indefinite"></animateTransform>
                            <animate attributeName="fill-opacity" keyTimes="0;1" dur="1s" repeatCount="indefinite" values="1;0" begin="-0.9s"></animate>
                        </circle>
                    </g>
                </g><g transform="translate(62.94427190999916,59.404564036679574)">
                    <g transform="rotate(36)">
                        <circle cx="0" cy="0" r="4" fill="#5B3DDE" fill-opacity="0.9" transform="scale(0.2 0.2)">
                            <animateTransform attributeName="transform" type="scale" begin="-0.8s" values="0 0;1 1" keyTimes="0;1" dur="1s" repeatCount="indefinite"></animateTransform>
                            <animate attributeName="fill-opacity" keyTimes="0;1" dur="1s" repeatCount="indefinite" values="1;0" begin="-0.8s"></animate>
                        </circle>
                    </g>
                </g><g transform="translate(54.94427190999916,65.21690426072246)">
                    <g transform="rotate(72)">
                        <circle cx="0" cy="0" r="4" fill="#5B3DDE" fill-opacity="0.8" transform="scale(0.1 0.1)">
                            <animateTransform attributeName="transform" type="scale" begin="-0.7s" values="0 0;1 1" keyTimes="0;1" dur="1s" repeatCount="indefinite"></animateTransform>
                            <animate attributeName="fill-opacity" keyTimes="0;1" dur="1s" repeatCount="indefinite" values="1;0" begin="-0.7s"></animate>
                        </circle>
                    </g>
                </g><g transform="translate(45.05572809000084,65.21690426072246)">
                    <g transform="rotate(108)">
                        <circle cx="0" cy="0" r="4" fill="#5B3DDE" fill-opacity="0.7" transform="scale(1.38767e-8 1.38767e-8)">
                            <animateTransform attributeName="transform" type="scale" begin="-0.6s" values="0 0;1 1" keyTimes="0;1" dur="1s" repeatCount="indefinite"></animateTransform>
                            <animate attributeName="fill-opacity" keyTimes="0;1" dur="1s" repeatCount="indefinite" values="1;0" begin="-0.6s"></animate>
                        </circle>
                    </g>
                </g><g transform="translate(37.05572809000084,59.404564036679574)">
                    <g transform="rotate(144)">
