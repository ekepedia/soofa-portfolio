                    <g transform="rotate(180)">
                        <circle cx="0" cy="0" r="4" fill="#5B3DDE" fill-opacity="0.5" transform="scale(0.8 0.8)">
                            <animateTransform attributeName="transform" type="scale" begin="-0.4s" values="0 0;1 1" keyTimes="0;1" dur="1s" repeatCount="indefinite"></animateTransform>
                            <animate attributeName="fill-opacity" keyTimes="0;1" dur="1s" repeatCount="indefinite" values="1;0" begin="-0.4s"></animate>
